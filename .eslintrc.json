{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": 0,
    "indent": [
        "error",
        2
    ],
    "no-return-assign": [
        "off"
    ],
    "react/jsx-one-expression-per-line": "off",
    "no-underscore-dangle": 0,
    "react/jsx-props-no-spreading": "off"

  }
}
